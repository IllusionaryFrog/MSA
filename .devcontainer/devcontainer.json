{
	"name": "Rust",
	"image": "mcr.microsoft.com/devcontainers/rust:1-1-bookworm",

	"mounts": [
		{
			"source": "cargo-cache-${devcontainerId}",
			"target": "/usr/local/cargo",
			"type": "volume"
		},
		{
			"source": "ssh-cache-frog",
			"target": "/home/vscode/.ssh",
			"type": "volume"
		},
		{
			"source": "/mnt/f/Media",
			"target": "/media",
			"type": "bind"
		}
	],

	"forwardPorts": [29839]
}